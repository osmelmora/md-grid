    "@kadira/storybook": "2.35.3",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.3.0",
    "babel-plugin-syntax-class-properties": "6.13.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
